{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/article\\/([^/]+?)\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/articles\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/articles\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/banana-is-the-best-meal-packages-in-the-UK-spark-outrage-online\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/become-a-morning-person-with-the-help-of-alarm-clock\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/how-to-make-toys-from-old-olarpaper\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/titulo-del-articulo\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/what-you-need-to-know-about-photography\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/why-a-balloon-is-a-flexible-bag\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/why-a-balloon-is-a-flexible-bag-that-can-be-inflated-with-a-gas\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/elements\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/articles\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/elements\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}